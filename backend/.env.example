# Environment Variables Template

# Database Configuration
# Update with your PostgreSQL credentials
DATABASE_URL="postgresql://postgres:password@localhost:5432/nebulalink?schema=public"

# JWT Secret (change in production)
JWT_SECRET="nebulalink_super_secret_jwt_key_change_in_production"

# Server Port
PORT=5000

# Tenor API Key (optional - demo key included in code)
TENOR_API_KEY="your_tenor_api_key_here"
